* {
  bg: #1A1A1A;
  bg-active: #F0F0F0;
  fg: #F0F0F0;
  fg-active: #1a1a1a;
  brdr: #5C5C5C;
}
element {
    padding: 15px ;
    cursor:  pointer;
    spacing: 5px ;
    border:  0;
		border-radius: 20;
}

element normal.normal {
    background-color: var(bg);
    text-color:       var(fg);
}
element normal.urgent {
    background-color: var(bg);
    text-color:       var(fg);
}
element normal.active {
    background-color: var(bg-active);
    text-color:       var(fg-active);
}
element selected.normal {
    background-color: var(bg-active);
    text-color:       var(fg-active);
}
element selected.urgent {
    background-color: var(bg-active);
    text-color:       var(fg-active);
}
element selected.active {
    background-color: var(bg-active);
    text-color:       var(fg-active);
}
element alternate.normal {
    background-color: var(bg);
    text-color:       var(fg);
}
element alternate.urgent {
    background-color: var(bg);
    text-color:       var(fg);
}
element alternate.active {
    background-color: var(bg-active);
    text-color:       var(fg-active);
}
element-text {
    background-color: transparent;
    cursor:           inherit;
    highlight:        inherit;
    text-color:       inherit;
}
element-icon {
    background-color: transparent;
    size:             1.0000em ;
    cursor:           inherit;
    text-color:       inherit;
}
window {
	width: 800px;
	background-color: transparent;
	border: none;
}
mainbox {
	background-color: var(bg);
	padding: 30px;
	border: 1px solid;
	border-color: var(brdr);
	border-radius: 20;
}
message {
    padding:      1px ;
    border-color: var(fg);
    border:       2px solid 0px 0px ;
}
textbox {
    text-color: var(fg);
}
listview {
    padding:      20px 0px 0px ;
    scrollbar:    false;
    border-color: var(fg);
    spacing:      2px ;
    fixed-height: 0;
    border:       none;
}
scrollbar {
    width:        4px ;
    padding:      0;
    handle-width: 8px ;
    border:       0;
    handle-color: var(fg);
}
sidebar {
    border-color: var(separatorcolor);
    border:       2px dash 0px 0px ;
}
button {
    cursor:     pointer;
    spacing:    0;
    text-color: var(normal-foreground);
}
button selected {
    background-color: var(selected-normal-background);
    text-color:       var(selected-normal-foreground);
}
num-filtered-rows {
    expand:     false;
    text-color: Gray;
}
num-rows {
    expand:     false;
    text-color: Gray;
}
textbox-num-sep {
    expand:     false;
    str:        "/";
    text-color: Gray;
}
inputbar {
    padding:    10px ;
    spacing:    0px ;
    text-color: var(fg);
}
case-indicator {
    spacing:    0;
    text-color: var(fg);
}
entry {
    text-color:        var(fg);
    cursor:            text;
    spacing:           0;
    placeholder-color: Gray;
    placeholder:       "Type to filter";
}
prompt {
    spacing:    0;
    text-color: var(fg);
}
textbox-prompt-colon {
    margin:     0px 0.3000em 0.0000em 0.0000em ;
    expand:     false;
    str:        ":";
    text-color: inherit;
}
